<div class="modal fade" id="errorModal" tabindex="-1" aria-labelledby="errorModalLabel" aria-hidden="true">
	<div class="modal-dialog modal-dialog-centered">
		<div class="modal-content">
			<div class="modal-header p-0 flex-column align-items-center justify-content-center border-0">
				<img class="rounded-circle my-4" src="~/images/closeBtn.png" asp-append-version="true">
				<h5 class="modal-title" id="errorModalLabel"></h5>
			</div>
			<div class="modal-footer p-0 my-3 border-0 align-items-center justify-content-center">
				<button type="button" class="text-white rounded-pill py-2 px-4 border-0" data-bs-dismiss="modal">OK
					!</button>
			</div>
		</div>
	</div>
</div>
<div class="modal fade" id="successModal" tabindex="-1" aria-labelledby="successModalLabel" aria-hidden="true">
	<div class="modal-dialog modal-dialog-centered">
		<div class="modal-content">
			<div class="modal-header p-0 flex-column align-items-center justify-content-center border-0">
				<img class="rounded-circle my-4" src="~/images/tickBig.png" asp-append-version="true">
				<h5 class="modal-title text-center" id="successModalLabel">Succesfully Logged In !!</h5>
			</div>
			<div class="modal-footer p-0 my-3 border-0 align-items-center justify-content-center">
				<button type="button" class="text-white rounded-pill py-2 px-4 border-0" data-bs-dismiss="modal">OK
					!</button>
			</div>
		</div>
	</div>
</div>
@if (!signInManager.IsSignedIn(User))
{
	<div class="modal fade" id="loginModal" aria-hidden="true" aria-labelledby="loginModalLabel" tabindex="-1">
		<div class="modal-dialog modal-dialog-centered justify-content-center">
			<div class="modal-content">
				<div class="modal-header p-0 border-0 mb-4">
					<h5 class="modal-title" id="loginModalLabel">Log In to your Account</h5>
					<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
				</div>
				<div class="modal-body p-0">
					<form class="loginModalForm" method="post">
						<div class="errorAlert alert fade d-none alert-danger px-3 py-2 d-flex align-items-center"
						role="alert">
							<div class="errorMessage"></div>
						</div>
						<input type="email" class="form-control" asp-for="Email" placeholder="Email" />
						<span class="text-danger d-block mt-1 mb-2 px-1" asp-validation-for="Email"></span>
						<input type="password" class="form-control" asp-for="Password" placeholder="Password" />
						<span class="text-danger d-block mt-1 mb-2 px-1" asp-validation-for="Password"></span>
						<label class="form-check-label mb-2" asp-for="RememberMe">
							<input class="form-check-input" type="checkbox" asp-for="RememberMe" />
							Remember Me
						</label>
						<button type="submit" class="text-white w-100 border-0 rounded-pill d-block">Login</button>
					</form>
					<a href="#" class="d-block mt-4 mb-1 text-center" data-bs-toggle="modal"
					data-bs-target="#forgetPasswordModal">Forget Password?</a>
					<div class="createAccountDiv">
						Don't have account?
						<a asp-controller="Account" asp-action="CustomerSignUp">Create an Account</a>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class="modal fade" id="forgetPasswordModal" aria-hidden="true" aria-labelledby="forgetPasswordModalLabel"
	tabindex="-1">
		<div class="modal-dialog modal-dialog-centered justify-content-center">
			<div class="modal-content">
				<div class="modal-header mb-4 p-0 border-0">
					<h5 class="modal-title" id="forgetPasswordModalLabel">Forget Password</h5>
					<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
				</div>
				<div class="modal-body p-0">
					<form class="forgetPasswordModalForm" asp-controller="Account"
					asp-action="SendForgetPasswordEmailToken">
						<input type="email" class="form-control" asp-for="Email" placeholder="Email" />
						<span class="d-block text-danger mt-1 mb-3 px-1" asp-validation-for="Email"></span>
						<button class="text-white w-100 border-0 mb-3 rounded-pill d-block">Send</button>
					</form>
					<a href="#" class="d-block text-center" data-bs-toggle="modal" data-bs-target="#loginModal">
						<img class="me-2" src="~/images/Shape 2.png" style="transform: rotate(180deg)" />
						Back to Login</a>
				</div>
			</div>
		</div>
	</div>
}
